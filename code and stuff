import statistics

#use whatever numbers you need
data = [1,2,3,4,5,5,91]

#this is all the math
mean =statistics.mean(data)
median =statistics.median(data)
mode =statistics.mode(data)
largest =max(mode*(data))
smallest =min(mode*(data))
added =sum(data)
subtracted=difference(data)
print(mean, mode, median, largest, smallest, added)
